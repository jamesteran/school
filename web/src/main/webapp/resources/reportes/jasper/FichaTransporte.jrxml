	<queryString>
		<![CDATA[SELECT
	DISTINCT ssi.std_id AS std_id,
	ssi.first_name AS std_nombres,
	ssi.last_name AS std_apellidos,
	ssi.std_blood_grp AS std_gruposangre,
	tra.transport_id AS tra_id,
	tra.std_id AS tra_stdid,
	tra.need_trans AS tra_tomatransporte,
	tra.pup_addr_morn AS tra_dirmañana,
	tra.pup_addr_noon AS tra_dirtarde,
	tra.trans_type AS tra_tipo,
	tra.trans_photo_morn AS tra_fotomañana,
	tra.trans_photo_noon AS tra_fototarde,
	tra.pup_coord_morn AS tra_coormañan,
	tra.pup_coord_noon AS tra_coortarde,
	COALESCE(papa.first_name,mama.first_name)AS rep_nombres, 
	COALESCE(papa.last_name,mama.last_name) AS rep_apellidos,
	COALESCE(papa.id_number,mama.id_number) AS rep_cedula,
	COALESCE(papa.parent_phone ,mama.parent_phone) AS rep_telefono,
	COALESCE(papa.parent_mobil,mama.parent_mobil) AS rep_celular
FROM
	sch_student_info ssi
LEFT  JOIN sch_transport tra ON
	ssi.std_id = tra.std_id
	LEFT JOIN sch_parent papa ON
	ssi.pha_id = papa.parent_id
	LEFT JOIN sch_parent mama ON
	ssi.mom_id = mama.parent_id
WHERE  
	ssi.std_id = $P{STD_ID}]]>
	</queryString>

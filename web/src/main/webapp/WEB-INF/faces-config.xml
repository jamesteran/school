   <!-- This descriptor activates the JSF 2.0 Servlet -->

   <!-- Write your navigation rules here. You are encouraged to use CDI for 
      creating @Named managed beans. -->
   <!-- Navigation rules  start-->
   <navigation-rule>
    <from-view-id>/*</from-view-id>    
    <navigation-case>
      <from-outcome>adminhome</from-outcome>
      <to-view-id>/paginas/admin/adminPanel.xhtml</to-view-id>
      <redirect>     
      </redirect>
    </navigation-case>
  </navigation-rule>
  <navigation-rule>
    <from-view-id>/*</from-view-id>    
    <navigation-case>
      <from-outcome>teacherhome</from-outcome>
      <to-view-id>/paginas/teacher/teacherHome.xhtml</to-view-id>
      <redirect>     
      </redirect>
    </navigation-case>
  </navigation-rule>
  <navigation-rule>
    <from-view-id>/*</from-view-id>    
    <navigation-case>
      <from-outcome>studenthome</from-outcome>
      <to-view-id>/paginas/student/studentHome.xhtml</to-view-id>
      <redirect>     
      </redirect>
    </navigation-case>
  </navigation-rule>
  <navigation-rule>
    <from-view-id>/index.xhtml</from-view-id>    
    <navigation-case>
      <from-outcome>logout</from-outcome>
      <to-view-id>/index.xhtml</to-view-id>
      <redirect>     
      </redirect>
    </navigation-case>
  </navigation-rule>
  
  <navigation-rule>
    <from-view-id>/paginas/admin/*</from-view-id>    
    <navigation-case>
      <from-outcome>logout</from-outcome>
      <to-view-id>/log_in.xhtml</to-view-id>
      <redirect>     
      </redirect>
    </navigation-case>
  </navigation-rule>
  <navigation-rule>
    <from-view-id>/public/*</from-view-id>
    <navigation-case>
      <from-outcome>logout</from-outcome>
      <to-view-id>/index.xhtml</to-view-id>
      <redirect>     
      </redirect>
    </navigation-case>
  </navigation-rule>
  <navigation-rule>    
    <from-view-id>/paginas/teacher/*</from-view-id>    
    <navigation-case>
      <from-outcome>logout</from-outcome>
      <to-view-id>/log_in.xhtml</to-view-id>
      <redirect>     
      </redirect>
    </navigation-case>
  </navigation-rule>
  <navigation-rule>
    <from-view-id>/paginas/student/*</from-view-id>
    <navigation-case>
      <from-outcome>logout</from-outcome>
      <to-view-id>/log_in.xhtml</to-view-id>
      <redirect>     
      </redirect>
    </navigation-case>
  </navigation-rule>
  <navigation-rule>
    <from-view-id>/paginas/admin/section.xhtml</from-view-id>
    <navigation-case>
      <from-outcome>marks</from-outcome>
      <to-view-id>/paginas/admin/marks.xhtml</to-view-id>
      <redirect>     
      </redirect>
    </navigation-case>
  </navigation-rule>      
   <!-- Navigation rules end-->
      
      
      
